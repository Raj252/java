import java.util.*;
import java.lang.*;
import java.io.*;
class Ideone
{
	public static void main(String[]args)
	{      
		int num;
		Scanner scan = new Scanner(System.in);
      
        num = scan.nextInt();
                       
                        if(num % 2==0)
                        {
		System.out.println("even");
                        }
		else 
		{
		System.out.println("odd");
		}

		
	}
}
